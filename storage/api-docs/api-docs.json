{
    "openapi": "3.0.0",
    "info": {
        "title": "Tinder API",
        "description": "API Documentation for Tinder App",
        "contact": {
            "email": "admin@example.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/",
            "description": "Local Server"
        }
    ],
    "paths": {
        "/api/liked": {
            "get": {
                "tags": [
                    "Liked"
                ],
                "summary": "Get list of liked people",
                "operationId": "0c833622c79852486b816e9cda769124",
                "parameters": [
                    {
                        "name": "per_page",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 10
                        }
                    },
                    {
                        "name": "user_id",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of liked people"
                    }
                }
            }
        },
        "/api/people/{id}/like": {
            "post": {
                "tags": [
                    "Reaction"
                ],
                "summary": "Like a person",
                "operationId": "fa11d9179c5e8070276ace0e13030e74",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Person ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": 1
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Liked successfully"
                    },
                    "404": {
                        "description": "Person not found"
                    }
                }
            }
        },
        "/api/people/{id}/dislike": {
            "post": {
                "tags": [
                    "Reaction"
                ],
                "summary": "Dislike a person",
                "operationId": "25b6dbd30caca054e2b1c81daa2334e4",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Person ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": 1
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Disliked successfully"
                    },
                    "404": {
                        "description": "Person not found"
                    }
                }
            }
        },
        "/api/recommendations": {
            "get": {
                "tags": [
                    "People"
                ],
                "summary": "Get list of recommended people",
                "operationId": "f0ac77661847d67e935d73ea3c9000f0",
                "parameters": [
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Number of items per page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 10
                        }
                    },
                    {
                        "name": "lat",
                        "in": "query",
                        "description": "Latitude for location filter",
                        "required": false,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "lng",
                        "in": "query",
                        "description": "Longitude for location filter",
                        "required": false,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "radius_km",
                        "in": "query",
                        "description": "Radius in km for location filter",
                        "required": false,
                        "schema": {
                            "type": "number",
                            "format": "float",
                            "default": 50
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of recommended people",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Person"
                                            }
                                        },
                                        "current_page": {
                                            "type": "integer"
                                        },
                                        "last_page": {
                                            "type": "integer"
                                        },
                                        "per_page": {
                                            "type": "integer"
                                        },
                                        "total": {
                                            "type": "integer"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/people/{id}": {
            "get": {
                "tags": [
                    "People"
                ],
                "summary": "Get a single person",
                "operationId": "071d181674956e8cd9193e7327782262",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Person ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Person object",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Person"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Person not found"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Person": {
                "title": "Person",
                "required": [
                    "id",
                    "name"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "John Doe"
                    },
                    "age": {
                        "type": "integer",
                        "example": 25
                    },
                    "latitude": {
                        "type": "number",
                        "format": "float",
                        "example": -6.2
                    },
                    "longitude": {
                        "type": "number",
                        "format": "float",
                        "example": 106.816666
                    },
                    "pictures": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "example": "https://example.com/picture.jpg"
                        }
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Liked",
            "description": "API endpoints for liked people"
        },
        {
            "name": "Reaction",
            "description": "API endpoints for liking/disliking people"
        },
        {
            "name": "People",
            "description": "People"
        }
    ]
}